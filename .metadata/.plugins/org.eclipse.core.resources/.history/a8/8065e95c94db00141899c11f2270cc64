#include <iostream>
#include "rabinhash.h"

using namespace std;
using namespace rabin;

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	RabinHash hashFunc;
	for (uint32_t i = 0; i < 256*256; i++){
		uint8_t * v = (uint8_t *) (&i);
		hashFunc.push(v, 4);
		//cout << hashFunc.fingerprint << endl;
		//cout << int(i) << endl;
	}
	return 0;
}
